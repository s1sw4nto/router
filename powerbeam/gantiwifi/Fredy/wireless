
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/ar934x_wmac'
	option noscan '1'
	option country 'US'
	option htmode 'HT40'
	option disabled '0'
	option channel '8'
	option txpower '18'

config wifi-iface
	option network 'wwan'
	option ssid 'Fredy'
	option device 'radio0'
	option mode 'sta'
	option bssid '28:C8:7C:E7:D5:52'
	option key 'matdiono14'
	option encryption 'psk2'
	option macaddr '34:E9:11:30:B5:ED'


